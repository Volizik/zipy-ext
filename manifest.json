{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ZipyFirewall",
  "description": "Zipy Firewall bypass",
  "version": "0.0.4",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon48.png"
  },
  "browser_action": {
    "default_title": "ZipyFirewall",
    "default_icon": "icon16.png"
  },
  "permissions": [
    "cookies",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "omnibox": {
    "keyword": "setcookie"
  },
  "manifest_version": 2
}